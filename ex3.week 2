# by using for loop
n =5
x=1
for i in range (1, n+1):
    if (i != n):
        print(i, end=" x ")
    else:
        print(i)
    x *=i
print()
print(x)

#by using while
i=1
while (True):
    if (i != n):
        print(i, end=" x ")
    else:
        print(i)
    x *=i
    i +=1
    if (i == n+1):
        break
print()
print(x)